<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.com.sourcesistemas.snipplet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AgregarCategoriaActivity"></activity>
        <activity android:name=".SnippletActivity"></activity>
        <activity android:name=".AdministrarNubeActivity"></activity>
        <activity android:name=".EditSnippletActivity"></activity>
        <activity android:name=".CreateNewSnippletActivity"></activity>
        <activity android:name=".PreferencesActivity"></activity>





    </application>

</manifest>